{
  "name": "artes-approval-system-backend",
  "version": "0.1.0",
  "description": "Backend para o sistema de aprovação de artes",
  "main": "src/server.js",
  "scripts": {
    "start": "node src/server.js",
    "dev": "nodemon src/server.js",
    "migrate": "knex migrate:latest",
    "seed": "knex seed:run",
    "test": "jest",
    "lint": "eslint src/**/*.js",
    "lint:fix": "eslint src/**/*.js --fix",
    "build": "echo \"No build step required for Node.js\"",
    "build:dev": "NODE_ENV=development npm run build",
    "build:test": "NODE_ENV=test npm run build",
    "build:prod": "NODE_ENV=production npm run build",
    "deploy:dev": "NODE_ENV=development npm run migrate && npm run start",
    "deploy:test": "NODE_ENV=test npm run migrate && npm run start",
    "deploy:prod": "NODE_ENV=production npm run migrate && npm run start",
    "setup:env": "node scripts/setup-env.js",
    "setup:dev": "node scripts/setup-env.js development",
    "setup:test": "node scripts/setup-env.js test",
    "setup:prod": "node scripts/setup-env.js production"
  },
  "keywords": [
    "artes",
    "aprovação",
    "api",
    "express",
    "node"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "cloudinary": "^1.41.0",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-async-errors": "^3.1.1",
    "helmet": "^7.1.0",
    "joi": "^17.11.0",
    "jsonwebtoken": "^9.0.2",
    "knex": "^3.0.1",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "sqlite3": "^5.1.6",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.0",
    "winston": "^3.11.0"
  },
  "devDependencies": {
    "eslint": "^8.53.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.1",
    "jest": "^29.7.0",
    "nodemon": "^3.0.1",
    "prettier": "^3.1.0",
    "supertest": "^6.3.3"
  }
} 